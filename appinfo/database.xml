<?xml version="1.0" encoding="UTF-8" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>
	
	<table>
		<name>*dbprefix*ldaporg_group_admins</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<autoincrement>1</autoincrement>
				<length>11</length>
			</field>
			<field>
				<name>group_id</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>255</length>
			</field>
			<field>
				<name>admin_id</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>255</length>
			</field>
		</declaration>
	</table>
</database>